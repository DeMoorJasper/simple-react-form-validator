{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "plugin:react/recommended",
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "rules": {
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "react/display-name": 0,
    "prettier/prettier": "error"
  }
}
